<h1>New Doctor Hire for <%=@hospital.name%></h1>

<%= form_tag "/hospitals/#{@hospital.id}/doctors" do %>

  <%= label_tag :name %>
  <%= text_field_tag :name %>

  <%= label_tag :specialty %>
  <%= text_field_tag :specialty %>

  <%= label_tag :education %>
  <%= text_field_tag :education %>

  <%= submit_tag 'Submit' %>

<% end %>
