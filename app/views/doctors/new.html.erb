<h1><%= "New Doctor Hire for #{@hospital.name}" %></h1>

<center>
  <%= form_for [:hospital, @doctor] do |f|  %>
    <%= f.label :name %>
    <%= f.text_field :name%>

    <%= f.label :specialty %>
    <%= f.text_field :specialty %>

    <%= f.label :education %>
    <%= f.text_field :education %>

    <%= f.submit %>
  <% end %>
</center>
